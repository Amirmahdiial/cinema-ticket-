
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./db.sqlite"
}


model Movie { 
  id                String     @default(cuid()) @id
  title             String
  description       String
  director          String
  genre             String
  rate              String
  background_image  String
  actor_image       String
  card_image        String
}